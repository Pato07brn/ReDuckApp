{
  "expo": {
    "name": "ReDuckApp",
    "slug": "ReDuckApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/favicon.png",
    "favicon" :"./assets/images/decaptado.ico",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2f2f2"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/favicon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "title": "ReDuckApp",
      "favicon": "./assets/images/decaptado.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
